{
	"name" : "sunhill/sunhill",
	"description" : "Das Sunhill-Framework",
	"keywords" : [
		"framework",
		"sunhill",
		"objects",
		"database"
	],
	"homepage" : "https://laravel-zero.com",
	"type" : "library",
	"license" : "MIT",
	"support" : {
		"issues" : "https://github.com/laravel-zero/laravel-zero/issues",
		"source" : "https://github.com/laravel-zero/laravel-zero"
	},
	"authors" : [{
			"name" : "Klaus Dimde",
			"email" : "klaus@dimde.info"
		}
	],
	"require" : {
		"php" : "^7.1.3",
		"illuminate/database" : "5.6.*",
		"illuminate/log" : "5.6.*"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^7.1",
		"laravel-zero/framework" : "*"
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "app/",
			"Sunhill\\Test\\" : [
				"tests/lib",
				"tests/objects"
			],
			"Sunhill\\Objects\\" : "lib/objects",
			"Sunhill\\" : "lib",
			"Tests\\" : "tests/lib",
			"Sunhill\\Properties\\" : "lib/properties",
			"Sunhill\\Validators\\" : "lib/validators",
			"Sunhill\\Storage\\" : "lib/storage",
			"Sunhill\\Utils\\" : "lib/utils"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Tests\\" : "tests/"
		}
	},
	"config" : {
		"preferred-install" : "dist",
		"sort-packages" : true,
		"optimize-autoloader" : true,
		"platform" : {
			"ext-posix" : "0"
		}
	},
	"scripts" : {
		"post-create-project-cmd" : "@php application app:rename"
	},
	"minimum-stability" : "dev",
	"prefer-stable" : true
}